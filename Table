#include<stdio.h>
#include<conio.h>

int main()

{
    int num , i , tab;
    printf("Enter the number of which table you want :");
    scanf("%d",&num);

    printf("\n Table of %d is:\n",num);
     for(i=1 ; i<=10 ; i++)
     {
         tab=num*i;
         printf("%d * %d = %d\n", num ,i, tab);
     }
    return 0;
}
