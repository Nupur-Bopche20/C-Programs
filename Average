#include<stdio.h>

int main()

{
    int n1,n2,n3,n4,n5,n6,n7,n8,n9,n10;
    int avg;
    printf("Enter the 10 numbers\n");
    scanf("%d%d%d%d%d%d%d%d%d%d",&n1,&n2,&n3,&n4,&n5,&n6,&n7,&n8,&n9,&n10);
    avg=(n1+n2+n3+n4+n5+n6+n7+n8+n9+n10)/10;
    printf("the average value is %d",avg);
    return 0;
}
